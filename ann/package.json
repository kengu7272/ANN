{
  "name": "ann",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -L ./server/server.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "mysql2": "^3.6.1",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "nodemon": "^3.0.1",
    "postcss": "latest",
    "tailwindcss": "latest",
    "typescript": "latest"
  },
  "nodemonConfig": {
    "watch": [
      "server/server.mjs",
      "src/"
    ],
    "ext": "tsx,css",
    "ignore": [
      "node_modules",
      ".next",
      ".docker",
      "docker-compose.yml"
    ],
    "execMap": {
      "tsx": "node --loader ts-node/esm --experimental-specifier-resolution=node"
    },
    "exec": "ts-node"
  }
}
